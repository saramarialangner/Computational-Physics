    "def f(x):\n",
    "    return 5*np.exp(-x)+x-5\n",
    "\n",
    "x1=3\n",
    "x2=6\n",
    "\n",
    "if f(x2)*f(x1)>0:\n",
    "    raise ValueError(\"f(x1) and f(x2) must have opposite signs\")\n",
    "\n",
    "while (abs(x1-x2)>10**(-6)):\n",
    "    x3=0.5*(x1+x2)\n",
    "        \n",
    "    if f(x3)*f(x1)>0:\n",
    "        x1=x3\n",
    "            \n",
    "    else:\n",
    "        x2=x3\n",
    "\n",
    "x_root=0.5*(x1+x2)\n",
    "        \n",
    "print ('x ≈',x_root)\n",
