    "x = 1.0 #starting guess\n",
    "c = 2.0 #exp factor\n",
    "i=1\n",
    "#w_neg=-0.8\n",
    "w=0.5\n",
    "\n",
    "iterations = []\n",
    "\n",
    "#Overrelaxation method\n",
    "\n",
    "\n",
    "while (abs(x-((1+w)*(1 - np.exp(-c*x))-w*x))>10**(-6)):\n",
    "        x=(1+w)*(1 - np.exp(-c*x))-w*x\n",
    "        \n",
    "        iterations.append(i)\n",
    "        i += 1\n",
    "        print(x)\n",
    "\n",
    "print(iterations[-1])\n"
